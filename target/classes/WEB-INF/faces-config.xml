<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<factory>
		<exception-handler-factory>
			br.projetoestoque.util.jsf.JsfExceptionHandlerFactory</exception-handler-factory>
	</factory>
	<application>
	
	<locale-config>
		<default-locale>pt_BR</default-locale>
	</locale-config>
	
		<resource-bundle>
			<base-name>btnName</base-name>
			<var>btn</var>
		</resource-bundle>
		
		<resource-bundle>
			<base-name>message</base-name>
			<var>msg</var>
		</resource-bundle>
		
		<message-bundle>br.projetoestoque.messageError</message-bundle>
	</application>

</faces-config>
